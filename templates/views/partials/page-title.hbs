<div class="container">	
	{{#if filters.category}}
	<h1 class="page-title"><a href="{{sectionURL section}}">{{section}}</a></h1>
	<h2 class="category-title">&nbsp;<span class="pipe">|</span>&nbsp;{{categoryTitle filters.category}}</h2>
	{{else}}
	<h1 class="page-title">{{section}}</h1>
	{{/if}}
</div>
