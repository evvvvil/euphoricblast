<div class="container">	
	<i class="glyphicon glyphicon-stop title-square"></i>
	{{#if filters.category}}
	<h1 class="page-title"><a href="{{sectionURL section}}">{{section}}</a></h1>
	<h2 class="category-title">&nbsp;<span class="pipe">|</span>&nbsp;{{data.category.name}}</h2>
	{{else}}
	<h1 class="page-title">{{section}}</h1>
	{{/if}}

</div>