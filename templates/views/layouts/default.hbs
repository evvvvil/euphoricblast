						</div>
					</div>
				</div>
			</div>
		</div>
		</div>
		<div id="{{bodyId}}">
			<!--
				Flash messages allow you to display once-off status messages to users, e.g. form
				validation errors, success messages, etc
			-->
			{{#if messages}}
			<div id="flash-messages" class="container">
				{{#if messages.info}}
				<div class="alert alert-info">
						{{{flashMessages messages.info}}}
				</div>
				{{/if}}
				{{#if messages.success}}
				<div class="alert alert-success">
						{{{flashMessages messages.success}}}
				</div>
				{{/if}}
				{{#if messages.warning}}
				<div class="alert alert-warning">
						{{{flashMessages messages.warning}}}
				</div>
				{{/if}}
				{{#if messages.error}}
				<div class="alert alert-danger">
						{{{flashMessages messages.error}}}
				</div>
				{{/if}}
			</div>
			{{/if}}
			<!-- The content block should contain the body of your template's content-->
			{{{body}}}
		</div>
			{{>footer}}
		<script src="/js/jquery/jquery-3.3.1.min.js"></script>		
		<script src="/js/bootstrap/bootstrap-3.3.5.min.js"></script>
		{{#if cloudinaryResponsive}}
			<!--/////////////////////-->
			<!--CLOUDINARY RESPONSIVE-->
			<!--/////////////////////-->
			<script src="/js/cloudinary-core-shrinkwrap.js" type="text/javascript"></script>
			<script src="/js/cloudinary-responsive.js" type="text/javascript"></script>
		{{/if}}				
		
		{{#if lightbox}}
			<!--////////-->
			<!--LIGHTBOX-->
			<!--////////-->
			<script src="/js/ekko-lightbox.min.js" type="text/javascript"></script>		
			<script src="/js/lightbox-control.js" type="text/javascript"></script>
		{{/if}}

		{{#if carousel}}
			<!--////////-->
			<!--CAROUSEL-->
			<!--////////-->
			<script src="/js/carousel-control.js" type="text/javascript"></script>
		{{/if}}
		{{#if pageReloader}}
			<!--/////////////-->
			<!--PAGE RELOADER-->
			<!--/////////////-->
			<script src="/js/page-reloader.js" type="text/javascript"></script>
		{{/if}}
		{{#if data.post.video}}
			{{#ifcontains data.post.video 'youtu'}}
			<!--//////////////////-->
			<!--YOUTUBE AUTOPLAYER-->
			<!--//////////////////-->
			<script src="/js/youtube-control.js" type="text/javascript"></script>
			{{else}}
				{{#ifcontains data.post.video 'vimeo'}}
				<!--////////////////-->
				<!--VIMEO AUTOPLAYER-->
				<!--////////////////-->			
				<script src="/js/vimeo-player.js"></script>
				<script src="/js/vimeo-control.js"></script>
				{{/ifcontains}}
			{{/ifcontains}}	
			<script src="/js/video-player-resizer.js" type="text/javascript"></script>			
		{{/if}}
		{{{isAdminEditorJS user}}}
	</body>
</html>
