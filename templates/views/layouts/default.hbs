<!DOCTYPE html>
<html>
	<head>
		
		<script async src="https://www.googletagmanager.com/gtag/js?id=UA-118755208-1"></script>
		<script>
		  window.dataLayer = window.dataLayer || [];
		  function gtag(){dataLayer.push(arguments);}
		  gtag('js', new Date());

		  gtag('config', 'UA-118755208-1');
		</script>

		<meta charset="utf-8">
		<meta name="viewport" content="width=device-width, initial-scale=1.0">
		<meta http-equiv="X-UA-Compatible" content="IE=edge">
		<!--<meta http-equiv="Cache-control" content="max-age=86400, public">-->
		
		<meta name="description" content="{{{getMetaDescription section filters.category data.post.title data.post.type data.post.content.brief}}}" />
		<meta name="keywords" content="{{{getMetaKeywords section filters.category data.post.title data.post.type data.post.content.brief}}}" />
		<meta name="author" content="Mangosh Prunier">
		<meta name="robots" content="index, follow">
		<meta name="revisit-after" content="2 days">
		<title>Euphoric Blast{{{pageTitle section filters.category data.post.title data.post.type}}}</title>
		<!-- interactive design,virtual reality,3d -->

		<link rel="apple-touch-icon" sizes="180x180" href="/apple-touch-icon.png?v=wAOJMa9XLG">
		<link rel="icon" type="image/png" sizes="32x32" href="/favicon-32x32.png?v=wAOJMa9XLG">
		<link rel="icon" type="image/png" sizes="16x16" href="/favicon-16x16.png?v=wAOJMa9XLG">
		<link rel="manifest" href="/site.webmanifest?v=wAOJMa9XLG">
		<link rel="mask-icon" href="/safari-pinned-tab.svg?v=wAOJMa9XLG" color="#333333">
		<link rel="shortcut icon" href="/favicon.ico?v=wAOJMa9XLG">
		<meta name="msapplication-TileColor" content="#ffffff">
		<meta name="theme-color" content="#ffffff">

		<meta property="og:image:width" content="1200">
		<meta property="og:image:height" content="628">
		<meta property="og:title" content="Euphoric Blast{{{pageTitle section filters.category data.post.title data.post.type}}}">
		<meta property="og:description" content="{{{getMetaDescription section filters.category data.post.title data.post.type data.post.content.brief}}}">
		<meta property="og:url" content="{{orginalURL}}">
		{{#if filters.category}}
			{{#ifgt data.featuredPosts.length 0}}
				{{#each data.featuredPosts}}
					{{#ifPostIsInCategoryKey categories ../filters.category}}
						{{#if mainImage.exists}}			
		<meta property="og:image" content="{{{cloudinaryUrl mainImage width='1200' height='628' q='70' crop='fill' gravity=mainImageCrop}}}">
						{{else}}
		<meta property="og:image" content="http://www.euphoricblast.com/images/og-image.jpg">
						{{/if}}	
					{{/ifPostIsInCategoryKey}}
				{{/each}}
			{{else}}
		<meta property="og:image" content="{{{getFacebookOGImage data.post.type (cloudinaryUrl data.post.mainImage width='1200' height='628' q='70' crop='fill' gravity=data.post.mainImageCrop)}}}">			
			{{/ifgt}}
		{{else}}
		<meta property="og:image" content="{{{getFacebookOGImage data.post.type (cloudinaryUrl data.post.mainImage width='1200' height='628' q='70' crop='fill' gravity=data.post.mainImageCrop)}}}">	
		{{/if}}
		<link href="/styles/site.min.css" rel="stylesheet">

		{{!-- This file provides the default styling for the KeystoneJS Content Editor
			- only renders when user is logged in and admin--}}
		{{{isAdminEditorCSS user}}}
		{{!-- HTML5 Shim and Respond.js IE8 support of HTML5 elements and media queries
		//- Safely remove this block if you are not targeting IE8 --}}
		<!--[if lt IE 9]>
			<script src="//cdn.jsdelivr.net/html5shiv/3.7.3/html5shiv.js"></script>
			<script src="//cdn.jsdelivr.net/respond/1.4.2/respond.min.js"></script>
		<![endif]-->
		{{#if fontAwesome}}
			<link href="/styles/font-awesome.min.css" rel="stylesheet">	
		{{/if}}
		{{#if lightbox}}
			<link href="/styles/ekko-lightbox.min.css" rel="stylesheet">
		{{/if}}		
	</head>
	<body>
		<header>
		<div id="header">
			<nav>
			<div class="container-fluid">
				<div class="row">
					<div role="navigation" class="navbar navbar-default navbar-polygon">
						<div class="container">
							<div class="navbar-header">
								<button type="button" data-toggle="collapse" data-target=".navbar-collapse" class="navbar-toggle">
									<span class="sr-only">Toggle navigation</span>
									<span class="icon-bar"></span><span class="icon-bar"></span><span class="icon-bar"></span>
								</button>
								<a href="/" class="navbar-double-link">
								<div class="navbar-logo-container"><div class="navbar-logo-container-rotate">{{>logo-svg}}</div></div>
								<div class="navbar-brand-container"><div class="navbar-brand">Euphoric <span class="brand-bold">Blast</span></div></div>
								</a>
							</div>
						
							<div class="collapse navbar-collapse">
								<ul class="nav navbar-nav navbar-left">
										{{# each navLinks}}
										<li {{#ifSectionOrProject ../section key ../data.post.type}}class="active"{{/ifSectionOrProject}}>
											<a href="{{ href }}">{{ label }}</a>
										</li>
										{{/each}}
								</ul>
								<!--<ul class="nav navbar-nav navbar-right navbar-social navbar-collapse">
									<li><a href="#" title="Follow Us On Facebook"><i class="fa fa-facebook"></i></a></li>
	      							<li><a href="#" title="Follow Us On Google Plus"><i class="fa fa-twitter"></i></a></li>
	      							<li><a href="https://www.youtube.com/channel/UCQKU0AE0r8wPtbwTFh846Hw" target="_blank" title="Follow Us On YouTube"><i class="fa fa-youtube"></i></a></li>
	      							<li><a href="#" title="Follow Us On Linked In"><i class="fa fa-vimeo"></i></a></li>
								</ul>-->
							</div>
						</div>
					</div>
				</div>
			</div>
		</nav>
	</div>
	</header>
		<div id="{{bodyId}}">
			{{#if messages}}
			<section id="section-flash-messages">
				<div id="flash-messages" class="container">
					{{#if messages.info}}
					<div class="alert alert-info">
							{{{flashMessages messages.info}}}
					</div>
					{{/if}}
					{{#if messages.success}}
					<div class="alert alert-success">
							{{{flashMessages messages.success}}}
					</div>
					{{/if}}
					{{#if messages.warning}}
					<div class="alert alert-warning">
							{{{flashMessages messages.warning}}}
					</div>
					{{/if}}
					{{#if messages.error}}
					<div class="alert alert-danger">
							{{{flashMessages messages.error}}}
					</div>
					{{/if}}
				</div>
			</section>
			{{/if}}
			
			{{{body}}}
		</div>
			{{>footer}}
		<script src="/js/jquery/jquery-3.3.1.slim.min.js"></script>		
		<script async src="/js/bootstrap/bootstrap-3.3.5.min.js"></script>
		{{#if cloudinaryResponsive}}
			<!--/////////////////////-->
			<!--CLOUDINARY RESPONSIVE-->
			<!--/////////////////////-->
			<script src="/js/cloudinary-core-shrinkwrap.min.js" type="text/javascript"></script>
			<script src="/js/cloudinary-responsive.js" type="text/javascript"></script>
		{{/if}}				
		
		{{#if lightbox}}
			<!--////////-->
			<!--LIGHTBOX-->
			<!--////////-->
			<script async src="/js/ekko-lightbox.min.js" type="text/javascript"></script>		
			<script async src="/js/lightbox-control.js" type="text/javascript"></script>
		{{/if}}

		{{#if carousel}}
			<!--////////-->
			<!--CAROUSEL-->
			<!--////////-->
			<script async src="/js/carousel-control.js" type="text/javascript"></script>
		{{/if}}
		{{#if pageReloader}}
			<!--/////////////-->
			<!--PAGE RELOADER-->
			<!--/////////////-->
			<!--<script src="/js/page-reloader.js" type="text/javascript"></script>-->
		{{/if}}
		{{#if data.post.video}}
			{{#ifcontains data.post.video 'youtu'}}
			<!--//////////////////-->
			<!--YOUTUBE AUTOPLAYER-->
			<!--//////////////////-->
			<script async src="/js/youtube-control.js" type="text/javascript"></script>
			{{else}}
				{{#ifcontains data.post.video 'vimeo'}}
				<!--////////////////-->
				<!--VIMEO AUTOPLAYER-->
				<!--////////////////-->			
				<script async src="/js/vimeo-player.js"></script>
				<script async src="/js/vimeo-control.js"></script>
				{{/ifcontains}}
			{{/ifcontains}}	
			<script async src="/js/video-player-resizer.min.js" type="text/javascript"></script>
		{{/if}}
		{{{isAdminEditorJS user}}}
	</body>
</html>
